<div class="building-ticket-type {{building_ticket_type}} building-ticket-type--status__{{ eventbrite.is_booking_available ? 'available' : 'not-available' }}">
	{% if eventbrite is not empty %}
		{% if eventbrite.no_eventbrite_defined %}
			<em></em>
		{% elseif eventbrite.is_booking_available == false and eventbrite.forced_booking_starts == true %}
			<button class="button btn btn-default disabled" disabled="disabled">Bookings open {{ eventbrite.booking_starts|date('jS F') }}</button>
		{% elseif eventbrite.is_booking_available == true %}
			<a href="{{ eventbrite.url }}" target="_blank" class="button btn btn-default">Book now</a>
		{% elseif eventbrite.is_booking_available == false %}
			<a href="{{ eventbrite.url }}" target="_blank" class="button btn btn-default disabled">Booked out</a>
		{% endif %}
	{% else %}
		{#<em>Cannot determine booking status. Please contact administrator.</em>#}
	{% endif %}
</div>
